{
  "name": "@gasbuddy/hydration",
  "version": "1.2.1",
  "description": "Configuration oriented object construction",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=babel-register test/*.js",
    "test-some": "tap --node-arg=-r --node-arg=babel-register",
    "cover": "cross-env BABEL_ENV=test tap --nyc-arg=--check-coverage --cov --coverage-report=lcov test/test_*.js",
    "lint": "eslint .",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "keywords": [
    "gasbuddy",
    "express",
    "serialization",
    "configuration"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-object-entries": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-preset-gasbuddy": "^1",
    "babel-register": "^6.16.3",
    "bluebird": "^3.4.6",
    "cross-env": "^3.1.3",
    "eslint": "^3.7.0",
    "eslint-config-gasbuddy": "^1",
    "tap": "^7"
  },
  "nyc": {
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 85,
    "include": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false
  }
}
